@using System.Security.Claims
@model stock_management_system.Models.M_AGF_TruckBinLaneModel
@{
    Layout = null;
}

<head>
    <style>
    </style>
</head>

<!-- Modal -->
<div class="modal fade" id="importtruckbinmanagament" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title w-100 text-center position-absolute font-weight-bold">追加</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>
            </div>             <!--Modal Header End-->
            <div class="modal-body">
                <form method="post" asp-controller="M_AGF_TruckBinLane" asp-action="Import" enctype="multipart/form-data" autocomplete="on"
                      data-ajax="true" data-ajax-update="formDetails">
                    @Html.AntiForgeryToken()
                    <span class="text-danger">@ViewData["Error"] </span>
                    <div class="modal-body m">
                        <div class="form-group">
                            <label>運送会社選択</label>
                            <select asp-for="TruckBinCode" asp-items="Model.TruckBinSelectList" name="TruckBinCode" class="form-control">
                                <option value="">選択してください</option>
                            </select>
                        </div>
                        <div class="form-group">
                            <label>レーン選択</label>
                            <select asp-for="LaneNo" asp-items="Model.LaneNoSelectList" name="LaneNo" class="form-control">
                                <option value="">選択してください</option>
                            </select>
                        </div>
                        <div class="form-group">
                            <label>グループ番号入力</label>
                            <input asp-for="@Model.LaneGroupID" name="LaneGroupID" class="form-control" id="lane_group_idEdit" type="text" value="" placeholder="入力してください"/>
                        </div>
                        <!--Modal Footer Start-->
                        <div class="modal-footer mt-5">
                            <button class="btn btn-success col" type="submit">保存</button>
                            <button class="btn btn-secondary col" type="button" onclick="ImportTruckBinCancelSubmit()">キャンセル</button>
                        </div>  <!--Modal Footer End-->
                    </div>
                </form>
            </div>             <!--Modal Body End-->
        </div>
    </div>
</div>
<!--Modal Body End-->
@*


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}

}*@